{
  "expo": {
    "name": "Oil Salary",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/oil.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alikk8k.boltexponativewind",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.alikk8k.boltexponativewind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/oil.png",
        "backgroundColor": "#6B46C1"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "/oil.png",
      "themeColor": "#6B46C1",
      "meta": {
        "apple": {
          "touchIcon": "/oil-192.png",
          "statusBarStyle": "default"
        },
        "description": "تطبيق إدارة الرواتب والحوافز والأرباح",
        "themeColor": "#6B46C1"
      },
      "manifest": {
        "name": "Oil Salary Manager",
        "short_name": "OilSalary",
        "start_url": "/",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#6B46C1",
        "orientation": "portrait",
        "icons": [
          {
            "src": "/oil-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any maskable"
          },
          {
            "src": "/oil.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68faefa1-e270-4429-9d1a-47421cd9c4c9"
      }
    }
  }
}
