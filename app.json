{
  "expo": {
    "name": "Oil Salary",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/oil.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alikk8k.boltexponativewind",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/oil.png",
      "manifest": "./manifest.json",
      "title": "Oil Salary Manager",
      "description": "تطبيق إدارة الرواتب والحوافز والأرباح",
      "icon": "./assets/images/oil.png",
      "serviceWorker": "./sw.js"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68faefa1-e270-4429-9d1a-47421cd9c4c9"
      }
    },
    "android": {
      "package": "com.alikk8k.boltexponativewind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/oil.png",
        "backgroundColor": "#6B46C1"
      }
    }
  }
}
